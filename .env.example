# Firebase Configuration
# Obtené estos valores en: https://console.firebase.google.com/project/YOUR_PROJECT_ID/settings/general
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your-project.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Gemini API
# Obtené tu API key en: https://makersuite.google.com/app/apikey
# VITE_GEMINI_KEY es para el frontend (si se usa)
VITE_GEMINI_KEY=your_gemini_api_key_here

# GEMINI_API_KEY es para Netlify Functions (backend)
GEMINI_API_KEY=your_gemini_api_key_here
